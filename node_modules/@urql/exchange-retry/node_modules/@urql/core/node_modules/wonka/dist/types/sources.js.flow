// @flow

import type { Source, Observer, Subject, TeardownFn } from "./types";
declare export function fromArray<T>(array: T[]): Source<T>;
declare export function fromValue<T>(value: T): Source<T>;
declare export function make<T>(
  produce: (observer: Observer<T>) => TeardownFn
): Source<T>;
declare export function makeSubject<T>(): Subject<T>;
declare export var empty: Source<any>;
declare export var never: Source<any>;
declare export function interval(ms: number): Source<number>;
declare export function fromDomEvent(
  element: HTMLElement,
  event: string
): Source<Event>;
declare export function fromPromise<T>(promise: Promise<T>): Source<T>;
